package uk.ac.ed.inf.aqmaps;

import java.util.ArrayList;

import com.mapbox.geojson.Polygon;

public class Navigation {
	private int made_moves;
	private PointDetails start_p;
	private ArrayList<Polygon> no_fly_polygons;
	private ArrayList<Sensor> sensors;
	private ArrayList<PointDetails> sensor_coords;
	Navigation(int mm, PointDetails sp, ArrayList<Polygon> nfp, ArrayList<Sensor> s, ArrayList<PointDetails> sc) {
		made_moves = mm;
		start_p = sp;
		no_fly_polygons = nfp;
		sensors = s;
		sensor_coords = sc;
	}
}
